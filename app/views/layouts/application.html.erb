<!DOCTYPE html>
<html>
<head>
  <title>Tracker</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  
<% if flash[:errors] %>
  <%= flash[:errors].join("<br>").html_safe %>
<% end %>

<%= current_user.username if logged_in? %>

<%= yield %>

</body>
</html>
